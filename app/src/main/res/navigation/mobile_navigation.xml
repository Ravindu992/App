<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.multilayoutuiandroidapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_Frame"
        android:name="com.example.multilayoutuiandroidapp.ui.Frame.FrameFragment"
        android:label="@string/title_Frame"
        tools:layout="@layout/fragment_frame" />

    <fragment
    android:id="@+id/navigation_form"
    android:name="com.example.multilayoutuiandroidapp.ui.form.FormFragment"
    android:label="Student Form"
    tools:layout="@layout/fragment_constraint" />

        <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.multilayoutuiandroidapp.ui.Dashboard.DashboardFragment"
